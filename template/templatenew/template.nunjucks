<!doctype html>
<html>
<head>
  <meta charset='utf-8'>
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="viewport" content="width=device-width">
  <title>PubPub API</title>
  <script src="jquery.min.js"></script>
  <script src='flatdoc_theme.js'></script>
  <link href='styles.css' rel='stylesheet'>
  <link href='https://fonts.googleapis.com/css?family=Inconsolata:400,700' rel='stylesheet' type='text/css'>
  <link href='https://fonts.googleapis.com/css?family=Merriweather:400italic' rel='stylesheet' type='text/css'>
  <!-- Meta -->
  <meta content="PubPub" property="og:title">
</head>

<body role='flatdoc' class="language-css">
  <div class='content-root'>
    <div class='menubar fade-in'>
      <div class='menu section' role='flatdoc-menu'>
        <a id="pubpub-design-guide-link" href="#pubpub-design-guide" class="level-1">PubPub API</a>
        <ul class="level-2" id="pubpub-design-guide-list">

        {% for chapter in documentation %}
          <li id="pubpub-design-guide-{{ chapter.title}}-item" class="level-2">
            <a id="pubpub-design-guide-{{ chapter.title}}-link" class="level-2" href="#{{ chapter.uniqueId }}">
                {{ chapter.title }}
            </a>
          </li>
        {% endfor %}

        {% for resource in resources %}
          {% include "./sidebar.nunjucks" %}
        {% endfor %}
      </div>
    </div>
    <div role='flatdoc-content' class='content fade-in two'>
      <h1 id="pubpub-design-guide">PubPub API</h1>
      <p>Version {{ version }}
        </br>
        {{ baseUri }}
      </p>
      {% for chapter in documentation %}
        <h2 id="{{ chapter.uniqueId }}">
          {{ chapter.title }}
        </h2>
        <div class="overview">
            {{ chapter.content }}
        </div>
        
      {% endfor %}
      <!--h2 id="pubpub-design-guide-typography">Routes</h2-->
      {% for resource in resources %}
        {% include "./resource.nunjucks" %}
      {% endfor %}
    </div>
  </div>
</body>
</html>
